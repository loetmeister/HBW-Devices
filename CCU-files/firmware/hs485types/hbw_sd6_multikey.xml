			</parameter>		
            <parameter id="SENSOR" hidden="true" operations="none">
                <logical type="address"/>
                <physical type="array">
                    <physical size="4.0" type="integer" interface="eeprom">
                        <address index="+0"/>
                    </physical>
                    <physical size="1.0" type="integer" interface="eeprom">
                        <address index="+4"/>
                    </physical>
                </physical>
            </parameter>
            <parameter id="CHANNEL" hidden="true" operations="none">
                <logical type="integer" min="0" max="255" default="255"/>
                <physical size="1.0" type="integer" interface="eeprom">
                    <address index="+5"/>
                </physical>
            </parameter>
            <parameter id="SHORT_ACTION_TYPE">
                <logical type="option">
                    <option id="ON"/>
                    <option id="OFF"/>
                    <option id="INACTIVE"/>
                    <option id="TOGGLE" default="true"/>
					<option id="BLINK_ON"/>
					<option id="BLINK_TOGGLE"/>
                </logical>
                <physical size="0.3" type="integer" interface="eeprom">
                    <address index="+6"/>
                </physical>
            </parameter>
            <parameter id="LONG_ACTION_TYPE">
                <logical type="option">
                    <option id="ON"/>
                    <option id="OFF"/>
                    <option id="INACTIVE"/>
                    <option id="TOGGLE" default="true"/>
					<option id="BLINK_ON"/>
					<option id="BLINK_TOGGLE"/>
				</logical>
                <physical size="0.3" type="integer" interface="eeprom">
                    <address index="+6.3"/>
                </physical>
            </parameter>
			<parameter id="SHORT_ON_LEVEL">
                <logical type="float" unit="%" default="100" max="100" min="0"/>
                <conversion type="float_integer_scale" factor="2"/>
                <physical size="1.0" type="integer" interface="eeprom">
                    <address index="+7"/>
                </physical>
            </parameter>
  		    <parameter id="SHORT_OFF_LEVEL">
                <logical type="float" unit="%" default="0" max="100" min="0"/>
                <conversion type="float_integer_scale" factor="2"/>
                <physical size="1.0" type="integer" interface="eeprom">
                    <address index="+8"/>
                </physical>
            </parameter>
			<parameter id="LONG_ON_LEVEL">
                <logical type="float" unit="%" default="100" max="100" min="0"/>
                <conversion type="float_integer_scale" factor="2"/>
                <physical size="1.0" type="integer" interface="eeprom">
                    <address index="+9"/>
                </physical>
            </parameter>
  		    <parameter id="LONG_OFF_LEVEL">
                <logical type="float" unit="%" default="0" max="100" min="0"/>
                <conversion type="float_integer_scale" factor="2"/>
                <physical size="1.0" type="integer" interface="eeprom">
                    <address index="+10"/>
                </physical>
            </parameter>
			<parameter id="BLINK_ON_TIME">
				<logical type="float" unit="s" default="1.0" max="25.0" min="0.1"/>
				<physical size="1.0" type="integer" interface="eeprom">
					<address index="+11"/>
				</physical>
				<conversion type="float_integer_scale" factor="10"/>
				<conversion type="integer_integer_map">
					<value_map to_device="false" from_device="true" parameter_value="10" device_value="0xff"/>
				</conversion>
			</parameter>
			<parameter id="BLINK_OFF_TIME">
				<logical type="float" unit="s" default="1.0" max="25.0" min="0.1"/>
				<physical size="1.0" type="integer" interface="eeprom">
					<address index="+12"/>
				</physical>
				<conversion type="float_integer_scale" factor="10"/>
				<conversion type="integer_integer_map">
					<value_map to_device="false" from_device="true" parameter_value="10" device_value="0xff"/>
				</conversion>
			</parameter>
			<parameter id="BLINK_QUANTITY">
			   <logical type="integer" default="0" min="1" max="255">
			     <special_value id="NOT_USED" value="0"/>
			   </logical>	   
               <physical size="1.0" type="integer" interface="eeprom">
                  <address index="+13.0"/>
               </physical>	  
			</parameter>
        </paramset>
		
		<paramset id="hmw_dim_ch_values" type="VALUES">
			<parameter id="LEVEL" operations="read,write,event" control="DIMMER.LEVEL">
				<logical type="float" unit="100%" default="0.0" max="1.0" min="0.0"/>
				<physical type="integer" interface="command" value_id="LEVEL">
					<set request="LEVEL_SET"/>
					<get request="LEVEL_GET" response="INFO_LEVEL"/>
					<event frame="INFO_LEVEL"/>
				</physical>
				<conversion type="float_integer_scale" factor="200"/>
			</parameter>
			<parameter id="WORKING" operations="read,event" ui_flags="internal">
				<logical type="boolean" default="false"/>
				<physical type="integer" interface="command" value_id="STATE_FLAGS">
					<get request="LEVEL_GET" response="INFO_LEVEL"/>
					<event frame="INFO_LEVEL"/>
					<event frame="ACK_STATUS"/>
				</physical>
				<conversion type="boolean_integer"/>
				<conversion type="integer_integer_map">
					<value_map device_value="0x00" parameter_value="0" />
					<value_map device_value="0x01" parameter_value="0" />
					<value_map device_value="0x02" parameter_value="0" />
					<value_map device_value="0x03" parameter_value="0" />
					<value_map device_value="0x04" parameter_value="0" />
					<value_map device_value="0x05" parameter_value="0" />
					<value_map device_value="0x06" parameter_value="0" />
					<value_map device_value="0x07" parameter_value="0" />	
					<value_map device_value="0x08" parameter_value="1" />
					<value_map device_value="0x09" parameter_value="1" />
					<value_map device_value="0x0A" parameter_value="1" />
					<value_map device_value="0x0B" parameter_value="1" />
					<value_map device_value="0x0C" parameter_value="1" />
					<value_map device_value="0x0D" parameter_value="1" />
					<value_map device_value="0x0E" parameter_value="1" />
					<value_map device_value="0x0F" parameter_value="1" />	
				</conversion>				
			</parameter>	
			<parameter id="STATE" operations="read,event" ui_flags="internal">
				<logical type="option">
					<option id="ON" default="true"/>
					<option id="OFF"/>
					<option id="TOGGLE"/>
					<option id="BLINK_ON"/>
					<option id="BLINK_OFF"/>
					<option id="UNDEFINED"/>
				</logical>
				<physical type="integer" interface="command" value_id="STATE_FLAGS">
					<get request="LEVEL_GET" response="INFO_LEVEL"/>
					<event frame="INFO_LEVEL"/>
					<event frame="ACK_STATUS"/>
				</physical>
				<conversion type="option_integer">
					<value_map device_value="0x00" parameter_value="0" />
					<value_map device_value="0x01" parameter_value="1" />
					<value_map device_value="0x02" parameter_value="2" />
					<value_map device_value="0x03" parameter_value="3" />
					<value_map device_value="0x04" parameter_value="4" />
					<value_map device_value="0x05" parameter_value="5" />
					<value_map device_value="0x06" parameter_value="5" />
					<value_map device_value="0x07" parameter_value="5" />	
					<value_map device_value="0x08" parameter_value="0" />
					<value_map device_value="0x09" parameter_value="1" />
					<value_map device_value="0x0A" parameter_value="2" />
					<value_map device_value="0x0B" parameter_value="3" />
					<value_map device_value="0x0C" parameter_value="4" />
					<value_map device_value="0x0D" parameter_value="5" />
					<value_map device_value="0x0E" parameter_value="5" />
					<value_map device_value="0x0F" parameter_value="5" />	
				</conversion>
			</parameter>
		</paramset>		
	</channel>	

	<channel index="25" count="6" physical_index_offset="-1" type="TEMPSENSOR"><!-- WEATHER -->
		<link_roles>
			<source name="WEATHER_T"/>
		</link_roles>
	    <paramset type="MASTER" id="hmw_temp_ch_master" address_step="6" address_start="0x40">
			<parameter id="SENSOR_ID">
			   <logical type="integer" default="255" max="254" min="1">
			       <!--<special_value id="NOT_USED" value="0"/> FHEM does not display two special_values-->
			       <special_value id="AUTO" value="255"/>
			   </logical>	   
               <physical size="1.0" type="integer" interface="eeprom">
                  <address index="+0"/>
               </physical>			   
			</parameter>
			<parameter id="SEND_DELTA_TEMP">
			   <logical type="float" unit="&#x2103;" default="0.5" min="0.1" max="25.0">
			       <special_value id="NOT_USED" value="0"/>
			   </logical>	   
               <physical size="1.0" type="integer" interface="eeprom">
                  <address index="+1.0"/>
               </physical>	
               <conversion type="float_integer_scale" factor="10"/>		
               <conversion type="integer_integer_map">
                 <value_map to_device="false" from_device="true" parameter_value="5" device_value="0xff"/>
               </conversion>			   
			</parameter>			
			<parameter id="SEND_MIN_INTERVAL">
			   <logical type="integer" unit="s" default="10" min="5" max="3600">
			     <special_value id="NOT_USED" value="0"/>
			   </logical>	   
               <physical size="2.0" type="integer" interface="eeprom" endian="little">
                  <address index="+2.0"/>
               </physical>	  
               <conversion type="integer_integer_map">
                 <value_map to_device="false" from_device="true" parameter_value="10" device_value="0xffff"/>
               </conversion>				   
			</parameter>
			<parameter id="SEND_MAX_INTERVAL">
			   <logical type="integer" unit="s" default="150" min="5" max="3600">
			     <special_value id="NOT_USED" value="0"/>
			   </logical>	   
               <physical size="2.0" type="integer" interface="eeprom" endian="little">
                  <address index="+4.0"/>
               </physical>	
               <conversion type="integer_integer_map">
                 <value_map to_device="false" from_device="true" parameter_value="150" device_value="0xffff"/>
               </conversion>				   
			</parameter>
		</paramset>
		<paramset type="VALUES" id="hmw_temp_input_values">
			<parameter id="TEMPERATURE" operations="read,event">
				<logical type="float" min="-273.15" max="327.67" unit="&#x2103;">
				  <special_value id="NOT_USED" value="-273.15"/>
				</logical>
				<physical type="integer" interface="command" value_id="TEMPERATURE">
					<get request="LEVEL_GET" response="TEMP_EVENT"/>
					<event frame="TEMP_EVENT"/>
				</physical>
				<conversion type="float_integer_scale" factor="100"/>
			</parameter>
		</paramset>		
	</channel>
	
